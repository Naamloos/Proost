{
  "compilerOptions": {
   "target": "esnext",
   "module": "esnext",
   "jsx": "react-jsx",
   "strict": true,
   "moduleResolution": "node",
   "esModuleInterop": true,
   "skipLibCheck": true,
   "forceConsistentCasingInFileNames": true,
   "baseUrl": "./",
   "paths": {
    "@/*": ["src/*"]
   }
  },
  "include": ["src", "*.d.ts", "vite.config.ts"],
}